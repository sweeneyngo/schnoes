{
  "name": "schnoes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --detectOpenHandles --runInBand",
    "prepare": "husky install",
    "format": "prettier --write \"{src,test}/**/*.{ts,tsx,scss,js,json}\"",
    "lint-conflict": "npx eslint-config-prettier src/pages/index.tsx"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "next/core-web-vitals",
      "prettier"
    ],
    "ignorePatterns": [],
    "rules": {
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "dependencies": {
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}
